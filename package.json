{
  "name": "dsp-api",
  "version": "1.0.0",
  "main": "dist/index.js",
  "scripts": {
    "start": "concurrently \"npm run dev\" \"npm run frontend:dev\"",
    "dev": "ts-node src/index.ts",
    "build": "tsc",
    "test": "echo \"Error: no test specified\" && exit 1",
    "frontend:install": "cd frontend && npm install",
    "frontend:dev": "cd frontend && npm run dev",
    "frontend:build": "cd frontend && npm run build",
    "dev:full": "concurrently \"npm run dev\" \"npm run frontend:dev\"",
    "ngrok": "ngrok http --url swine-relaxing-leopard.ngrok-free.app 3000",
    "vercel-build": "tsc && mkdir -p public && cp -r dist public/dist",
    "start:prod": "node dist/index.js",
    "cancel-subscription": "ts-node src/scripts/cancel-subscription.ts",
    "list-subscription-plans": "ts-node src/scripts/list-subscription-plans.ts",
    "create-subscription": "ts-node src/scripts/create-subscription-direct.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "description": "API using Square for payments/subscriptions and Supabase for authentication and database",
  "dependencies": {
    "@mailchimp/mailchimp_marketing": "^3.0.80",
    "@supabase/supabase-js": "^2.49.3",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.1",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.13.14",
    "@types/uuid": "^10.0.0",
    "axios": "^1.9.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "crypto-js": "^4.2.0",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "form-data": "^4.0.2",
    "gsap": "^3.12.7",
    "multer": "^1.4.5-lts.2",
    "node-fetch": "^3.3.2",
    "square": "^42.3.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.2",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.8",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "concurrently": "^8.2.2"
